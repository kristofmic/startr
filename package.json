{
  "name": "startr",
  "version": "1.0.0",
  "main": "www",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean && yarn run build:prod && yarn run copy",
    "build:prod": "NODE_ENV=production webpack --config ./client/webpack.config.js --bail",
    "build:dev": "NODE_ENV=development webpack --progress --colors --config ./client/webpack.config.js",
    "build:dev:watch": "yarn build:webpack && CONFIG=development webpack -w --progress --colors --config ./public/webpack.config-compiled.js",
    "clean": "rm -rf ./public && rm -rf ./node_modules/.cache/babel-loader",
    "copy": "yarn copy:images",
    "copy:images": "mkdir -p ./public/images && cp -r ./client/images/* ./public/images",
    "format": "prettier --config ./.prettierrc --write 'client/**/*.js' 'client/**/*.jsx' 'server/**/*.js'",
    "lint": "eslint --fix .",
    "upload": "./scripts/s3_upload --directory ./public --bucket supermtg",
    "secrets:decrypt": "node ./scripts/secrets -d",
    "secrets:encrypt": "node ./scripts/secrets -e",
    "start:dev:secrets": "source ./conf/secrets/decrypted/.env && PORT=8081 NODE_ENV=development CONFIG=development nodemon -e js,json,jsx ./www | bunyan -o short",
    "start:server:dev": "source ./conf/secrets/decrypted/.env && PORT=8081 NODE_ENV=development CONFIG=development SKIP_WP=true nodemon -e js,json ./www | bunyan -o short",
    "start:dev": "PORT=8080 NODE_ENV=development CONFIG=development nodemon -e js,json,jsx ./www | bunyan -o short"
  },
  "dependencies": {
    "@babel/register": "7.7.0",
    "aws-sdk": "2.569.0",
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "bootstrap": "4.3.1",
    "bunyan": "1.8.12",
    "classnames": "2.2.6",
    "compression": "1.7.4",
    "ejs": "2.7.1",
    "express": "4.17.1",
    "express-request-id": "1.4.1",
    "helmet": "3.21.2",
    "lodash": "4.17.19",
    "meow": "5.0.0",
    "nconf": "0.10.0",
    "pretty-hrtime": "1.0.3",
    "prop-types": "15.7.2",
    "qs": "6.9.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-transition-group": "4.3.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@babel/runtime": "7.7.2",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "3.2.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "file-loader": "4.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.13.0",
    "nodemon": "1.19.4",
    "prettier": "1.19.1",
    "react-dev-utils": "9.1.0",
    "sass-loader": "8.0.0",
    "terser-webpack-plugin": "2.2.1",
    "url-loader": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-manifest-plugin": "2.2.0"
  }
}
